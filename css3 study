$(function(){
    var winH = $(window).height();

    $(window).on("scroll",function(){
        (funCtrl(function(){
            if($(window).scrollTop()+2*winH/3 >= $("#mod6").offset().top){
                $("#mod6").addClass("hover");
            }
            if($(window).scrollTop()+2*winH/3 >= $("#mod10").offset().top){
                $("#mod10").addClass("hover");
            }
            if($(window).scrollTop()+2*winH/3 >= $("#country").offset().top){
                $("#country").addClass("hover");
            }
        },100,300)())
    });
});
</script>
