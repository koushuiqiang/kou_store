<script>
    $(function () {
        //导航预定下拉
        jK.drop({
            btn: $("#yd_menu"),
            cont: $("#yd_menu>dl"),
            way: "mouseenter"
        });
        //回到顶部
        jK.goTop($("#goTop"));

        //加入收藏
        function AddFavorite(title, url) {
            try {
                window.external.addFavorite(url, title);
            }
            catch (e) {
                try {
                    window.sidebar.addPanel(title, url, "");
                }
                catch (e) {
                    alert("抱歉，您所使用的浏览器无法完成此操作。nn加入收藏失败，请使用Ctrl+D进行添加");
                }
            }
        }
        $(".floatAddFav").click(function () {
            AddFavorite('签证宝', "http://www.visabao.com");
        });

    });
</script>
<!--共用脚本end-->
